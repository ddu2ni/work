doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title 사용자 리스트 목록
    link(rel='stylesheet', type='text/css', href='../public1/data/semantic.min.css')
    link(rel='stylesheet', type='text/css', href='../public1/data/listUserResp.css')
    script(src='https://code.jquery.com/jquery-3.1.1.min.js')
    script(src='./data/semantic.min.js')
  body
    .container
      br
      .ui.raised.segment
        a.ui.blue.ribbon.label 사용자 리스트 (jade)
        br
        br
        .ui.two.column.grid
          - for (var i = 0; i < result.length; i++)
            .column
              .ui.card
                .image
                  img(src='../public1/image/suzi.png')
                .content
                  a.header= result[i].name
                  .meta
                    span.date= moment(result[i].created).format('YYYY년 MM월 DD일')
                  .description
                    br
                    p 키: 168cm
                    p 직업: 가수
                .extra.content
                  | #{i + 100} Friends
